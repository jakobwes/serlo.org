{
  "name": "@serlo/editor-renderer",
  "version": "0.1.2",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:babel && yarn build:docker",
    "build:babel": "cross-env NODE_ENV=production babel src --extensions \".ts,.tsx\" --out-dir dist --root-mode upward --source-maps",
    "build:docker": "ts-node scripts/build",
    "prebuild:gcf": "rimraf dist-gcf",
    "build:gcf": "cross-env NODE_ENV=production webpack",
    "start": "nodemon --watch src --exec \"ts-node src/index.ts\""
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@serlo/editor": "^0.5.0",
    "@serlo/editor-helpers": "^0.0.0",
    "@serlo/legacy-editor-to-editor": "^0.0.0",
    "babel-runtime": "^6.0.0",
    "body-parser": "^1.0.0",
    "express": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/styled-components": "^4.0.0",
    "nodemon": "^1.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0"
  },
  "private": true
}
